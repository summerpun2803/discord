{
    "version": 2,
    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node",
        "config": {
            "commands": [
              "npm install --force"
            ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }
  